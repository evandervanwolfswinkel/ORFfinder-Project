ORF tabel
  ORF_ID 
  ORF_start
  ORF_stop

Sequentie tabel
  Sequentie_ID
  Sequentie_header
  Sequentie

vul EERST de sequentie tabel, dan de orf tabel

INSERT INTO ORF
    (ORF_ID, ORF_start, ORF_stop, Sequentie_Sequentie_ID)
    VALUES
    (1, 15, 789, 1)
    
INSERT INTO sequentie
(Sequentie_ID, Sequentie_header, Sequentie)
VALUES (1, '>Dit is een test sequentie','ATCGTAGCT')
